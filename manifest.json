{
    "name": "LeetDirect",
    "description": "No more procrastination!",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "storage", 
        "activeTab", 
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}